TimedFreeRoundBonusManager.prototype.UpdateTimer=function(){if(this.bonusRoundsData!=null){var clientServerDifference=this.currentTime-this.bonusRoundsData.RoundsLeft;if(clientServerDifference>2)this.currentTime=this.bonusRoundsData.RoundsLeft}var elapsedTime=Math.floor(this.currentTime);if(elapsedTime<0)elapsedTime=0;var seconds=Math.floor(elapsedTime%60).toString();var minutes=Math.floor(elapsedTime/60%60).toString();if(seconds.length<2)seconds="0"+seconds;if(minutes.length<2)minutes="0"+minutes;
