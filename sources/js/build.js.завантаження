GUIMessageResultWin.prototype.ComputeText=function(amount,multipliers,subject,usePaysText){var ret=[];if(subject!=null)ret.push(subject);if(usePaysText)ret.push(this.localized_Pays.text);if(multipliers!=null){var totalMultiplier=1;for(var i=0;i<multipliers.length;i++)totalMultiplier*=multipliers[i];if(totalMultiplier>1){ret.push(this.FormatValue(amount/totalMultiplier));for(var i=0;i<multipliers.length;i++)if(multipliers[i]>1){ret.push("x");ret.push(String(multipliers[i]))}ret.push("=")}}ret.push(this.FormatValue(amount));
