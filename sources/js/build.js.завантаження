ValuesCollectorVisual.prototype.GetCollectingDuration=function(valuesToCollect){var duration=0;for(var i=0;i<valuesToCollect.length;i++)if(valuesToCollect[i]>0)duration+=this.collectedValue[i].delayAfter;duration+=this.showDuration;duration+=this.hideDuration;return duration};
