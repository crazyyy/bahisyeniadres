XTVariable2CAT.prototype.CallbackFloat=function(param){this.DoIt(this.less,param<this.valueFloat);this.DoIt(this.lessOrEquals,param<=this.valueFloat);this.DoIt(this.equals,param==this.valueFloat);this.DoIt(this.notEquals,param!=this.valueFloat);this.DoIt(this.greaterOrEquals,param>=this.valueFloat);this.DoIt(this.greater,param>this.valueFloat)};
