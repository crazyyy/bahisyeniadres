AnimatedParticleFrames.prototype.OnUpdateParticle=function(particle){particle._epm_apf_frameTime+=Time.deltaTime;var newFrame=particle._epm_apf_frame;while(particle._epm_apf_frameTime>1/this.FPS){particle._epm_apf_frameTime-=1/this.FPS;newFrame++}newFrame%=this.frameInfo.length;if(newFrame!=particle._epm_apf_frame){var index=-1;var pppc=particle.parent.pixiZOrderedContainer.children;for(var i=0;i<pppc.length;i++)if(pppc[i]==particle.sprite)index=i;particle._epm_apf_frame=newFrame;particle.sprite=
