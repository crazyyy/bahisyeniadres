gameSymbol,null);EventManager.Trigger(ResourceEvents.evtRequestResources,gameSymbol)};VideoSlotsConnectionXTLayer.prototype.OnServerBalanceUpdated=function(data){this.SetBalance(data.Balance,false);this.SetBonusBalance(data.BonusBalance)};VideoSlotsConnectionXTLayer.prototype.OnBatterySaverStateChanged=function(val){UHTEngine.SetBatterySaverState(val)};VideoSlotsConnectionXTLayer.prototype.OnSettingsUpdated=function(settings){this.UpdateSettings(true,settings);this.settingsReceived=true;if(this.CanTriggerInitReady())this.TriggerInitReady()};
