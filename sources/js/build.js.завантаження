(function(pixi_spine){var core;(function(core){var Texture=function(){function Texture(image){this._image=image}Texture.prototype.getImage=function(){return this._image};Texture.filterFromString=function(text){switch(text.toLowerCase()){case "nearest":return TextureFilter.Nearest;case "linear":return TextureFilter.Linear;case "mipmap":return TextureFilter.MipMap;case "mipmapnearestnearest":return TextureFilter.MipMapNearestNearest;case "mipmaplinearnearest":return TextureFilter.MipMapLinearNearest;
