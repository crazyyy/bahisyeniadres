GameProtocolBonusGameParser.ParseMap=function(dict){var ret=new VsBonusGameData.Map;var wof_set=GameProtocolCommonParser.ParseIfExist(dict,"wof_set");var wof_mask=GameProtocolCommonParser.ParseIfExist(dict,"wof_mask");var wof_mi=GameProtocolCommonParser.ParseIfExist(dict,"wof_mi");var wof_p=GameProtocolCommonParser.ParseIfExist(dict,"wof_p");var wof_map=GameProtocolCommonParser.ParseIfExist(dict,"wof_map");ret.WOF_Values=[];ret.WOF_Mask=[];ret.WOF_Map=[];if(!_string.IsNullOrEmpty(wof_mi))if(wof_mi.length>
