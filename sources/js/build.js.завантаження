function CooldownOneShot(){this.clip=null;this.cooldown=0;this.cooldownTimer=-1}CooldownOneShot.prototype.UpdateTimer=function(){if(this.cooldownTimer>0)this.cooldownTimer-=Time.deltaTime};goog.require("UHT.XTLink");goog.require("UHT.SoundManager");goog.require("UHT.AudioClassesUtils");var VSSoundLogicVars={VSSoundLogicObject:"VSSoundLogicObject"};VS_SoundLogic.prototype=Object.create(XTLink.prototype);VS_SoundLogic.prototype.constructor=VS_SoundLogic;
