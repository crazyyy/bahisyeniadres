(function(pixi_spine){var core;(function(core){var SkeletonBounds=function(){function SkeletonBounds(){this.minX=0;this.minY=0;this.maxX=0;this.maxY=0;this.boundingBoxes=new Array;this.polygons=new Array;this.polygonPool=new core.Pool(function(){return core.Utils.newFloatArray(16)})}SkeletonBounds.prototype.update=function(skeleton,updateAabb){if(skeleton==null)throw new Error("skeleton cannot be null.");var boundingBoxes=this.boundingBoxes;var polygons=this.polygons;var polygonPool=this.polygonPool;
