ParticleSystemShapeBox.prototype.constructor=ParticleSystemShape;function ParticleSystemShapeBox(){this.boxX=1;this.boxY=1;this.boxZ=1}ParticleSystemShapeBox.prototype.generateNewPosition=function(){return new UHTMath.Vector3(this.boxX*(Math.random()-.5),this.boxY*(Math.random()-.5),this.boxZ*(Math.random()-.5))};ParticleSystemShapeBox.prototype.deserialize=function(src){this.boxX=Number(src.boxX);this.boxY=Number(src.boxY);this.boxZ=Number(src.boxZ)};
