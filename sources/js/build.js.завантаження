Renderer.prototype.init=function(){renderCanvas=document.createElement("canvas");renderCanvas.style.width="100%";renderCanvas.style.height="100%";this.checkWindowSize();this.resizeUHTScreen();var mustPreserveDrawingBuffer=false;var forceCanvas=navigator.userAgent.indexOf("forceCanvas")>-1;var parser=new UAParser2;var device=parser.getDevice();if(device.vendor!=undefined&&device.model!=undefined){var vendor=device.vendor;var model=device.model;if(vendor!="Apple")mustPreserveDrawingBuffer=true;for(var j=
