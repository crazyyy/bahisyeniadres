function ScrollableList(){Component.call(this);this.itemHeight=0;this.mouseWheelItems=0;this.buttonItems=0;this.items=null;this.itemValues=null;this.scrollbar=null;this.scrollbarThumb=null;this.clickThreshold=0;this.clickEvents=null;this.hasExtraItem=false;this.numberOfExtraItemsToAdd=0;this.enabledContent=null;this.enabledContents=null;this.enabledColliders=null;this.hasInertia=false;this.inertiaCurve=null;this.useMinValueReached=false;this.minValueReached=null;this.minValueNotReached=null;this.strictMinValue=
