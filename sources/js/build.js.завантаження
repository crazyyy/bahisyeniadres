SymbolSorterManager.prototype.SortSymbols=function(){if(this.rm!=null)for(var rIndex=0;rIndex<this.rm.reels.length;rIndex++){var symbolIndex=0;for(var sIndex=0;sIndex<this.rm.reels[rIndex].sortedSymbolHolders.length;sIndex++){this.vs=this.rm.reels[rIndex].sortedSymbolHolders[sIndex].currentSymbol;if(this.vs!=null){this.sps=this.vs.GetComponent(SymbolPanelSorter);if(this.sps!=null){switch(this.sortOrder){case SymbolSorterManager.SortOrder.topLeft:this.newDepth=rIndex*this.reelHolderOffset+symbolIndex*
this.symbolHoldersOffset;break;case SymbolSorterManager.SortOrder.topRight:this.newDepth=(this.rm.reels.length-rIndex-1)*this.reelHolderOffset+symbolIndex*this.symbolHoldersOffset;break;case SymbolSorterManager.SortOrder.bottomLeft:this.newDepth=rIndex*this.reelHolderOffset+(this.rm.reels[rIndex].sortedSymbolHolders.length-symbolIndex-1)*this.symbolHoldersOffset;break;case SymbolSorterManager.SortOrder.bottomRight:this.newDepth=(this.rm.reels.length-rIndex-1)*this.reelHolderOffset+(this.rm.reels[rIndex].sortedSymbolHolders.length-
symbolIndex-1)*this.symbolHoldersOffset;break;default:this.newDepth=0;break}this.sps.UpdateDepth(this.newDepth)}symbolIndex++}}}};goog.require("UHT.FOXLink");var WinLimitVars={WinLimit_TotalBetMultiplier:"WinLimit_TotalBetMultiplier",WinLimit_TotalAmount:"WinLimit_TotalAmount",WinLimit_UncappedWinLines:"WinLimit_UncappedWinLines"};WinLimitFOX.prototype=Object.create(FOXLink.prototype);WinLimitFOX.prototype.constructor=WinLimitFOX;
