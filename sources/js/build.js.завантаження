Renderer.prototype.resizeUHTScreen=function(){var PixelRatio=window.devicePixelRatio||window.screen.deviceXDPI/window.screen.logicalXDPI;if(UHT_DEVICE_TYPE.DESKTOP==true)if(PixelRatio>1.5)PixelRatio=1.5;if(PixelRatio>2)PixelRatio=2;UHTScreen.width=Math.floor(rememberedWindowWidth*PixelRatio);UHTScreen.height=Math.floor(rememberedWindowHeight*PixelRatio);if(createjs.BrowserDetect.isIOS){document.body.style.width=rememberedWindowWidth+"px";if(UHT_UA_INFO.browser.name=="Chrome"){renderCanvas.style.width=
