GameWindow.prototype.OnShowWindow=function(){if(!this.isEnabled)return;if(this.waitForBigWin&&XT.GetBool(Vars.WaitInResultForBigWin))this.showAfterBigWinClear=true;else if(this.waitForWinCount&&XT.GetBool(Vars.LastWinIsCounting))this.showAfterWinCount=true;else this.ShowWindow()};
GameWindow.prototype.OnHideWindow=function(){if(this.cat!=null&&this.hideEventId.cat!=null){if(this.showEventId!=null&&this.showEventId.IsRunning())this.showEventId.Stop();this.cat.RegisterCallback(this.hideEventId.id,this.HideEventFinished,this);this.cat.StartEvent(this.hideEventId.id)}else{this.gameObject.SetActive(false);for(var i=0;i<this.triggerAfterHide.length;i++)XT.TriggerEvent(this.triggerAfterHide[i].name)}};
