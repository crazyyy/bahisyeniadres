GameProtocolCommonParser.ParseDouble=function(nameValues,paramName){return _number.otod(nameValues[paramName])};GameProtocolCommonParser.ParseDoubleIfExists=function(nameValues,paramName){if(nameValues[paramName]==undefined)return-1;return _number.otod(nameValues[paramName])};GameProtocolCommonParser.ParseNulableDouble=function(nameValues,paramName){var result=null;var buf=nameValues[paramName];if(buf!=undefined)if(_string.Trim(buf).length>0&&buf!="null")result=_number.otod(buf);return result};
