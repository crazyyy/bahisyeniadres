VS_SoundLogic.prototype.OnSoundLogicReelStopReceived=function(){this.reelIndices=XT.GetObject(Vars.LastStoppedReelIndices);this.reelIndex=this.reelIndices[this.reelIndices.length-1];this.stillPossibleWinComboBonus=XT.GetObject(Vars.StillPossibleWinComboBonus);this.stillPossibleWinComboScatter=XT.GetObject(Vars.StillPossibleWinComboScatter);this.stillPossibleWinComboWild=XT.GetObject(Vars.StillPossibleWinComboWild);this.stillPossibleWinComboPerSymbol=XT.GetObject(Vars.StillPossibleWinComboPerSymbol);
