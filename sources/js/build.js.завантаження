SetLines:function(lines){if(!XT.GetBool(Vars.HasAnteBet))XT.SetInt(Vars.BetToTotalBetMultiplier,lines);else XT.SetInt(Vars.BetToTotalBetMultiplier,XT.GetBool(Vars.GameHasWaysInsteadOfLines)?XT.GetInt(Vars.TotalNumberOfLines):lines);XT.SetInt(Vars.Lines,XT.GetBool(Vars.GameHasWaysInsteadOfLines)?XT.GetInt(Vars.TotalNumberOfLines):lines)}};function BonusRoundRetData(){this.isBonusRounds=false;this.messageIsOpened=false}goog.require("UHT.Engine");goog.require("UHT.UHTStage");StageSpin.prototype=Object.create(UHTStage.prototype);
