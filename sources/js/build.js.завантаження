VS_SpinExciter_Casanova.prototype.SimulateStopsOnReel=function(reelIdx){var reels=this.reelsManager.reels.length;var i;var j;var rIndex;for(var lineIdx=0;lineIdx<this.betLines.length;lineIdx++){var wli=this.CloneWinLineInfo(this.betLines[lineIdx]);var winningsymbol=2;var forceEndTheLine=false;var passedCurrentReel=false;for(i=0;i<wli.Positions.length;i++){if(reelIdx==(wli.isRightToLeft?wli.Positions.length-1-i:i)){passedCurrentReel=true;continue}if(this.ReelStopsAfter(wli.isRightToLeft?wli.Positions.length-
1-i:i,reelIdx))forceEndTheLine=true;var pos=wli.Positions[i];var reelIndex=pos%reels;var reelPosIdx=Math.floor(pos/reels);var symbol=this.GetSymbolFromReel(reelPosIdx,reelIndex);if(symbol!=2||forceEndTheLine)if(forceEndTheLine||symbol!=winningsymbol&&winningsymbol!=2||symbol<2){var newLength=passedCurrentReel?i:0;var newPos=_array.create(newLength,0);for(j=0;j<newLength;j++)newPos[j]=wli.Positions[j];wli.Positions=newPos;break}else winningsymbol=symbol;else this.StillPossibleWinComboWild[i]=true;
if(!this.betLines[lineIdx].containsWild)this.betLines[lineIdx].containsWild=symbol==2}this.betLines[lineIdx].dominatingSymbol=winningsymbol;if(winningsymbol>1)if(this.symbolPaying[winningsymbol]<=wli.Positions.length){this.SetReelShouldExcite(reelIdx);if(this.hasLoopExciteImportant)this.ExcitedImportantSymbols[reelIdx][winningsymbol]=true;if(this.payoutData!=null)this.AccumulatedPossibleWin[reelIdx]+=this.payoutData[winningsymbol][wli.Positions.length-1]*XT.GetDouble(Vars.CurrentWinMultiplier);var lineIsPaying=
false;if(this.symbolPaying[winningsymbol]<=(wli.isRightToLeft?this.betLines[lineIdx].Positions.length-1-reelIdx:reelIdx))lineIsPaying=true;for(i=0;i<wli.Positions.length;i++){if(reelIdx==(wli.isRightToLeft?this.betLines[lineIdx].Positions.length-1-i:i)){lineIsPaying=false;continue}if(lineIsPaying)this.symbolsShouldPlayWin[reelIdx].push(wli.Positions[i]);this.symbolsShouldPlayExcite[reelIdx].push(wli.Positions[i])}}}if(this.ScatterPays==SpecialSymbolPayType.Everywhere)this.SimulateReelStopForScatteredSymbol(1,
reelIdx);for(i=0;i<this.scatteredSymbols.length;i++)this.SimulateReelStopForScatteredSymbol(this.scatteredSymbols[i],reelIdx)};
VS_SpinExciter_Casanova.prototype.SimulateReelStopForScatteredSymbol=function(symbolId,reelIdx){var yetVisible=0;var symbolsOnReels=0;var reelsNotStoppedYet=0;var isRealScatterSymbol=symbolId==1;var lines=XT.GetInt(Vars.Lines);this.initialSymbolList=XT.GetObject(Vars.InitialResultScreenSymbols);var weHaveInitialSymbols=this.initialSymbolList!=null&&this.initialSymbolList.length>0&&!this.forcedFinalSymbolsOnScattersComputation;for(var i=0;i<this.reelsManager.reels.length;i++){if(this.SymbolsPresentAnywhereOnReel(symbolId,
i)<=0)continue;symbolsOnReels+=weHaveInitialSymbols?this.InitialSymbolAppearencesOnReel(symbolId,i):this.SymbolAppearencesOnReel(symbolId,i);if(i==reelIdx||this.ReelStopsAfter(i,reelIdx)){reelsNotStoppedYet++;continue}yetVisible+=this.SymbolAppearencesOnReel(symbolId,i)}if(isRealScatterSymbol){if(this.ScatterIsStacked){reelsNotStoppedYet=0;for(var i=0;i<this.reelsManager.reels.length;i++)if((i==reelIdx||this.ReelStopsAfter(i,reelIdx))&&this.SymbolsPresentAnywhereOnReel(symbolId,i)>0)reelsNotStoppedYet+=
this.reelsManager.reels[i].symbolHolders.length}if(yetVisible+reelsNotStoppedYet>=this.GetScatterMinCombo())this.StillPossibleWinComboScatter[reelIdx]=true;var maxScattersOnNextReel=this.ScatterIsStacked?this.reelsManager.reels[reelIdx].symbolHolders.length:1;if(this.SymbolsPresentAnywhereOnReel(symbolId,reelIdx)>0&&yetVisible>=this.GetScatterMinCombo()-maxScattersOnNextReel){this.AccumulatedPossibleWin[reelIdx]+=Math.ceil(this.payoutThreshold*(this.optimizeWaysComputation&&XT.GetBool(Vars.GameHasWaysInsteadOfLines)?
XT.GetInt(Vars.BetToTotalBetMultiplier):lines));this.SetReelShouldExcite(reelIdx)}if(symbolsOnReels>=this.GetScatterMinCombo())for(var j=0;j<this.ShouldAnimateScatter.length;j++)this.ShouldAnimateScatter[j]=true}else{var symbolIndex=_array.indexOf(symbolId,this.scatteredSymbols);var symbolsOnCurrentReel=weHaveInitialSymbols?this.InitialSymbolAppearencesOnReel(symbolId,reelIdx):this.SymbolAppearencesOnReel(symbolId,reelIdx);if(yetVisible+symbolsOnCurrentReel+(reelsNotStoppedYet-1)*this.possibleScatteredSymbolsOnReel[symbolIndex]>=
this.scatteredSymbolsMinCombo[symbolIndex])this.StillPossibleWinComboPerSymbol[reelIdx][symbolId]=true;if(this.scatteredSymbolsTriggerExcitement[symbolIndex])if(this.SymbolsPresentAnywhereOnReel(symbolId,reelIdx)>0)if(yetVisible>=this.scatteredSymbolsMinCombo[symbolIndex]-this.possibleScatteredSymbolsOnReel[symbolIndex]){this.AccumulatedPossibleWin[reelIdx]+=Math.ceil(this.payoutThreshold*(this.optimizeWaysComputation&&XT.GetBool(Vars.GameHasWaysInsteadOfLines)?XT.GetInt(Vars.BetToTotalBetMultiplier):
lines));this.SetReelShouldExcite(reelIdx)}}};VS_SpinExciter_Casanova.prototype.SetReelShouldExcite=function(reelIdx){var reelStopOrder=XT.GetObject(Vars.ReelsStopOrder);for(var rIndex=0;rIndex<reelStopOrder.length;rIndex++)if(reelStopOrder[rIndex]==reelIdx){if(rIndex>=this.firstExcitedReel)this.reelShouldExcite[reelIdx]=true;break}};VS_SpinExciter_Casanova.prototype.SimulateStops=function(){this.ResetArrays();for(var i=0;i<this.reelsManager.reels.length;i++)this.SimulateStopsOnReel(i)};
VS_SpinExciter_Casanova.prototype.SimulateStops_Ways=function(){this.ResetArrays();var i;var j;if(this.payoutData!=null){if(this.symbolPresenceOnReel_Ways.length==0)for(i=0;i<this.payoutData.length;i++){this.symbolPresenceOnReel_Ways.push([]);for(j=0;j<this.payoutData[i].length;j++)this.symbolPresenceOnReel_Ways[i].push(0)}else for(i=0;i<this.symbolPresenceOnReel_Ways.length;i++)for(j=0;j<this.symbolPresenceOnReel_Ways[i].length;j++)this.symbolPresenceOnReel_Ways[i][j]=0;var symbolID=0;for(i=0;i<
this.symbolList.length;i++)for(j=0;j<this.symbolList[i].length;j++){symbolID=this.symbolList[i][j];if(this.SymbolIsWild(symbolID))this.symbolPresenceOnReel_Ways[2][i]++;else this.symbolPresenceOnReel_Ways[symbolID][i]++}for(i=0;i<this.reelsManager.reels.length;i++)this.SimulateStopsOnReel_Ways(i)}};
VS_SpinExciter_Casanova.prototype.SimulateStopsOnReel_Ways=function(reelIdx){var rightToLeftWay=false;var i;var j;for(var symbolID=2;symbolID<this.symbolPresenceOnReel_Ways.length;symbolID++){if(!this.HasPayout(symbolID))continue;var forceEndTheLine=false;var passedCurrentReel=false;var winWaysLength=this.symbolPresenceOnReel_Ways[symbolID].length;for(i=0;i<this.symbolPresenceOnReel_Ways[symbolID].length;i++){var r2lDependentIdx=rightToLeftWay?this.symbolPresenceOnReel_Ways[symbolID].length-1-i:i;
if(reelIdx==r2lDependentIdx){passedCurrentReel=true;continue}if(this.ReelStopsAfter(r2lDependentIdx,reelIdx))forceEndTheLine=true;if(forceEndTheLine||this.symbolPresenceOnReel_Ways[symbolID][r2lDependentIdx]==0&&this.symbolPresenceOnReel_Ways[2][r2lDependentIdx]==0){winWaysLength=passedCurrentReel?i:0;break}else if(this.symbolPresenceOnReel_Ways[2][r2lDependentIdx]>0)this.StillPossibleWinComboWild[i]=true}if(this.symbolPaying[symbolID]<=winWaysLength){this.SetReelShouldExcite(reelIdx);if(this.hasLoopExciteImportant)this.ExcitedImportantSymbols[reelIdx][symbolID]=
true;var noOfWays=1;for(i=0;i<winWaysLength;i++){var idx=rightToLeftWay?this.symbolPresenceOnReel_Ways[symbolID].length-1-i:i;var presenceOnIdx=this.symbolPresenceOnReel_Ways[symbolID][idx];if(idx==reelIdx)presenceOnIdx=this.symbolList[reelIdx].length;else if(symbolID!=2)presenceOnIdx+=this.symbolPresenceOnReel_Ways[2][idx];noOfWays*=presenceOnIdx}if(this.payoutData!=null)this.AccumulatedPossibleWin[reelIdx]+=noOfWays*this.payoutData[symbolID][winWaysLength-1]*XT.GetDouble(Vars.CurrentWinMultiplier);
var lineIsPaying=false;if(this.symbolPaying[symbolID]<=(rightToLeftWay?this.symbolPresenceOnReel_Ways[symbolID].length-1-reelIdx:reelIdx))lineIsPaying=true;for(i=0;i<winWaysLength;i++){if(reelIdx==(rightToLeftWay?this.symbolPresenceOnReel_Ways[symbolID].length-1-i:i)){lineIsPaying=false;continue}if(lineIsPaying)for(j=0;j<this.symbolList[i].length;j++)if(this.symbolList[i][j]==symbolID||this.symbolList[i][j]==2)this.symbolsShouldPlayWin[reelIdx].push(j*this.symbolList.length+i);for(j=0;j<this.symbolList[i].length;j++)if(this.symbolList[i][j]==
symbolID||this.symbolList[i][j]==2){if(lineIsPaying)this.symbolsShouldPlayWin[reelIdx].push(j*this.symbolList.length+i);this.symbolsShouldPlayExcite[reelIdx].push(j*this.symbolList.length+i)}}}if(XT.GetBool(Vars.LinesPaysBothWays)){rightToLeftWay=!rightToLeftWay;if(rightToLeftWay)symbolID--}}if(this.ScatterPays==SpecialSymbolPayType.Everywhere)this.SimulateReelStopForScatteredSymbol(1,reelIdx);for(i=0;i<this.scatteredSymbols.length;i++)this.SimulateReelStopForScatteredSymbol(this.scatteredSymbols[i],
reelIdx)};VS_SpinExciter_Casanova.prototype.HasPayout=function(symbolID){for(var i=0;i<this.payoutData[symbolID].length;i++)if(this.payoutData[symbolID][i]>0)return true;return false};
VS_SpinExciter_Casanova.prototype.UpdateBetLines=function(){var lines=XT.GetInt(Vars.Lines);var reels=this.symbolList.length;var linesPaysBothWays=XT.GetBool(Vars.LinesPaysBothWays);this.betLines=_array.create(linesPaysBothWays?lines*2:lines,null);for(var i=0;i<this.betLines.length;i++){var isRightToLeft=i>=lines;this.betLines[i]=new WinLineInfo_Casanova;this.betLines[i].Id=i+1;this.betLines[i].Positions=_array.create(reels,0);this.betLines[i].containsWild=false;this.betLines[i].isRightToLeft=isRightToLeft;
for(var j=0;j<reels;j++)if(isRightToLeft)this.betLines[i].Positions[j]=this.linesDef.lines[i-lines].symbolIndices[reels-1-j];else this.betLines[i].Positions[j]=this.linesDef.lines[i].symbolIndices[j]}};VS_SpinExciter_Casanova.prototype.ComputePossibleWins=function(){if(this.optimizeWaysComputation&&XT.GetBool(Vars.GameHasWaysInsteadOfLines))this.SimulateStops_Ways();else{this.UpdateBetLines();this.SimulateStops()}this.UpdateActualWinCombo();this.SetWinComboSounds()};
VS_SpinExciter_Casanova.prototype.Excite=function(reelToStop){if(!XT.GetObject(Vars.ReelsCanBeExcited)[reelToStop])return false;var enoughMoney=false;if(this.payoutData==null)enoughMoney=true;else{var lines=XT.GetInt(Vars.Lines);if(this.AccumulatedPossibleWin[reelToStop]>=this.payoutThreshold*(this.optimizeWaysComputation&&XT.GetBool(Vars.GameHasWaysInsteadOfLines)?XT.GetInt(Vars.BetToTotalBetMultiplier):lines))enoughMoney=true}return this.reelShouldExcite[reelToStop]&&enoughMoney};
VS_SpinExciter_Casanova.prototype.UpdateSymbol=function(rsh,reelIdx,allowScatterExcite){var symbolId=rsh.currentSymbol.symbolId;var scatterIsWild=XT.GetBool(Vars.ScatterSymbolIsWild);if(!this.CanHighlightOff(symbolId)&&!(scatterIsWild&&symbolId==1))return;var orderedIdx=rsh.GetOrderedSymbolIdx();var foundExcited=false;var foundWinning=false;var i;for(i=0;i<this.symbolsShouldPlayExcite[reelIdx].length;i++)if(orderedIdx==this.symbolsShouldPlayExcite[reelIdx][i])foundExcited=true;for(i=0;i<this.symbolsShouldPlayWin[reelIdx].length;i++)if(orderedIdx==
this.symbolsShouldPlayWin[reelIdx][i])foundWinning=true;if(foundWinning&&foundExcited)rsh.currentSymbol.SetWinExcited();else if(foundWinning)rsh.currentSymbol.SetWinning();else if(foundExcited)rsh.currentSymbol.SetExcited();else if(!this.CanHighlightOff(symbolId)&&(scatterIsWild&&symbolId==1))rsh.currentSymbol.SetNormal();else rsh.currentSymbol.SetHighlightOff()};
VS_SpinExciter_Casanova.prototype.ReelStopped=function(reelIdx,isStoppingFast){if(isStoppingFast)return;var reelStopOrder=XT.GetObject(Vars.ReelsStopOrder);var i=0;for(i=0;i<reelStopOrder.length;i++)if(reelIdx==reelStopOrder[i])if(i<this.firstExcitedReel-1)return;var atLeastOneExcitedReel=false;var nextReelIdx=0;var nextReelIdxInOrder=0;for(i=0;i<reelStopOrder.length;i++)if(reelIdx==reelStopOrder[i]){if(i==reelStopOrder.length-1)nextReelIdxInOrder=i;else nextReelIdxInOrder=i+1;nextReelIdx=reelStopOrder[nextReelIdxInOrder]}for(i=
0;i<=nextReelIdxInOrder;i++)if(this.Excite(reelStopOrder[i]))atLeastOneExcitedReel=true;if(atLeastOneExcitedReel)for(i=0;i<reelStopOrder.length;i++){var reel=this.reelsManager.reels[reelStopOrder[i]];for(var j=0;j<reel.symbolHolders.length;j++)this.UpdateSymbol(reel.symbolHolders[j],nextReelIdx,false);if(reelStopOrder[i]==reelIdx)break}};
VS_SpinExciter_Casanova.prototype.InitialSymbolAppearencesOnReel=function(symbolId,reelidx){var ret=0;for(var i=0;i<this.initialSymbolList[reelidx].length;i++)if(this.initialSymbolList[reelidx][i]==symbolId)ret++;return ret};
VS_SpinExciter_Casanova.prototype.UpdateActualWinCombo=function(){var winLines=XT.GetObject(Vars.ReceivedWinLines);if(winLines==null||winLines.length==0)return;var longestWLine=winLines[0];var i;for(i=0;i<winLines.length;i++)if(longestWLine.WinPositions.length<winLines[i].WinPositions.length)longestWLine=winLines[i];var winPos=longestWLine.WinPositions;for(i=0;i<winPos.length;i++)this.ActualWinCombo[winPos[i]%this.reelsManager.reels.length]=true};goog.require("UHT.Engine");
var ReelFXReelStopEvent={ReelStopStarted:0,ReelStopEnded:1};ReelFX.prototype=Object.create(XTLink.prototype);ReelFX.prototype.constructor=ReelFX;function ReelFX(){XTLink.call(this);this.reelIndex=0;this.reelStopEvent=ReelFXReelStopEvent.ReelStopStarted;this.reelExcited=null;this.reelStop=null;this.reelStopWin=null;this.fastPlayReelExcited=null;this.fastPlayReelStop=null;this.fastPlayReelStopWin=null;this.reelStoppedFast=null;this.reelsExcited=null;this.numberOfReels=0}
