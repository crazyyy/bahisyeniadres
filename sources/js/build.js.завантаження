ParticleSystem.prototype.OnEnable=function(){if(this.playOnAwake){this.time=0;this.Clear(false);this.resetBursts()}this.subEmitors=this.gameObject.GetComponent("ParticleSystemExtraData");if(this.playOnAwake)this.Play();if(this.prewarm){var step=1/15;var adunator=0;while(adunator<this.duration){this.internalUpdate(step);adunator+=step}}};ParticleSystem.prototype.Start=function(){};
ParticleSystem.prototype.Awake=function(){this.epm=this.gameObject.GetComponents(ExtraParticleModule);for(var i=0;i<this.epm.length;i++)if(!this.epm[i].SupportsFastParticleContainer())this.fastContainer=false;if(this.fastContainer){this.pixiZOrderedContainer=new PIXI.ParticleContainer(500,{scale:true,position:true,rotation:true,uvs:false,alpha:true},500);this.pixiZOrderedContainer.blendMode=this.blendMode}else this.pixiZOrderedContainer=new PIXI.Container};ParticleSystem.prototype.OnDisable=function(){};
