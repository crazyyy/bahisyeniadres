this);this.eventHighlightOff.RegisterCallback(this.Callback_EventEnded,this);this.eventSpinStart.RegisterCallback(this.Callback_EventEnded,this);this.eventSpinLoop.RegisterCallback(this.Callback_EventEnded,this);this.eventSpinStop.RegisterCallback(this.Callback_EventEnded,this);this.eventExcited.RegisterCallback(this.Callback_EventEnded,this);this.eventWinExcited.RegisterCallback(this.Callback_EventEnded,this);this.eventSpinLoopExcited.RegisterCallback(this.Callback_EventEnded,this);this.eventSpinLoopExcitedImportant.RegisterCallback(this.Callback_EventEnded,
