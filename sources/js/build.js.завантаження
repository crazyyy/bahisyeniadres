GameProtocolCommonParser.ParseNulableInt(nameValues,GameProtocolDictionary.BonusGame.bonusGameType);var result=new VsBonusGameData;result.BonusTable.Status=GameProtocolCommonParser.ParseIntList(nameValues,GameProtocolDictionary.BonusGame.status);result.BonusTable.Wins=GameProtocolCommonParser.ParseDoubleList(nameValues,GameProtocolDictionary.BonusGame.wins);result.BonusTable.WinsMask=GameProtocolBonusGameParser.ParseWinsMask(nameValues,GameProtocolDictionary.BonusGame.winsMask);result.BonusTable.BGItemValues=
