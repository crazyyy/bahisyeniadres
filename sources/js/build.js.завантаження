this.extraLayout.rules.SetVisible(false);for(var i=0;i<this.secondaryExtraLabels.length;i++)this.secondaryExtraLabels[i].text=this.announcements[i].description;this.secondaryExtraDisplayer.UpdateTournament(this.uid);this.secondaryExtraRules.SetVisible(false)}if(this.styleSwitcher!=null)this.styleSwitcher.SwitchByDisplayStyle(this.uid);PromotionsHelper.PromotionCheckTournamentOptOut(this.uid);if(!this.showMerged){this.catShow.Start();this.isVisible=true;this.announcements.splice(0,1)}else{this.secondaryCatShow.Start();
