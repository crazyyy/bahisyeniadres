VS_MusicDuckByOneShot.prototype.XTInitVariablesAndEvents=function(){XT.SetObject(Vars.MusicDuckByOneShotObject,this);this.oneShotDuckers.push(this.winCounter)};VS_MusicDuckByOneShot.prototype.OnLastWinCountingChanged=function(counting){if(counting)this.OneShotStartsPlaying(null,true);else this.OneShotStartsPlaying(null,false)};VS_MusicDuckByOneShot.prototype.OnSoundManagerObject=function(sMgr){this.soundManager=sMgr};
