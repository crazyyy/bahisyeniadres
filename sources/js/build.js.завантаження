true)};var GA_SENT_REPLAY_OPENED=false;ReplayConnection.prototype.RequestWinnings=function(){if(!GA_SENT_REPLAY_OPENED){globalTracking.SendEvent("uht_behaviour_replay","Ingame_replay_opened_window",1,"BehaviourTracker");GA_SENT_REPLAY_OPENED=true}var req=new ServerRequest;req.Url=this.winningsURL;req.Handler=this.winningsResponseHandler;req.Method="GET";RequestManager.AddRequest(req)};
