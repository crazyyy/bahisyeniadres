CustomDragObject.prototype.OnMouseWheel=function(e){e.preventDefault();if(!this.gameObject.activeInHierarchy||!this.isHover||Globals.InputBlocked&&!(this.cachedCamera!=null&&this.cachedCamera.ignoreInputBlocked))return;this.wheelDirection=e.wheelDelta?-e.wheelDelta:e.detail;this.wheelDirection=UHTMath.clamp(this.wheelDirection,-1,1)};
