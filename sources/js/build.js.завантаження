SpecialSymbolPayType.OnActivePaylineLeftToRight)break;if(bonuses>=this.BonusMinCombo){this.ActualWinCombo[i]=true;this.ActualWinComboBonus[i]=true;for(var j=0;j<=i;j++)this.ShouldAnimateBonus[wli.Positions[j]]=true}if(bonuses+possibleBonuses>=this.BonusMinCombo)this.StillPossibleWinComboBonus[i]=true}}if(this.ScatterPays==SpecialSymbolPayType.Everywhere){var scatters=0;for(var i=0;i<reels;i++){var moreScattersPossible=false;var possibleScatters=0;var scatterIsOnReel=false;for(var j=i;j<reels;j++)if(this.SymbolsPresentAnywhereOnReel(1,
