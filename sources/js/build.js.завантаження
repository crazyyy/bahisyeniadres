AdditionalFreeSpins.prototype.FindInitialSymbolsWithID=function(id){var ret=[];var initialResultSymbols=XT.GetObject(Vars.InitialResultScreenSymbols);for(var i=0;i<initialResultSymbols.length;i++)for(var j=0;j<initialResultSymbols[i].length;j++)if(id==initialResultSymbols[i][j])ret.push(i+(initialResultSymbols[i].length-1-j)*initialResultSymbols.length);return ret};
