GUI_SoundLogic.prototype.Update=function(){if(this.enableSoundsWhenReady)if(UHTEngine.SoundsAreLoaded()){this.enableSoundsWhenReady=false;if(XT.GetObject(Vars.SoundState).gameSoundIsOn==false)XT.TriggerEvent(Vars.Evt_DataToCode_Pressed_SoundBtn)}};goog.require("UHT.XTLink");CoinManager.prototype=Object.create(XTLink.prototype);CoinManager.prototype.constructor=CoinManager;function CoinManager(){XTLink.call(this)}CoinManager.instance=null;CoinManager.isInited=false;CoinManager.lastBet=-1;
