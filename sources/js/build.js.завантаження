this.currentFontSize/this.fontSize;poc.style.font=""+this.currentFontSize+"px "+this.fontName;ignored=poc.height;outputText=poc.wordWrap(this._text);lines=outputText.split(/(?:\r\n|\r|\n)/);var fit2=lines.length-(defaultLines.length-1)<=this.maxLines;found2=fit2&&min2>=max2-1;if(fit2)min2=this.currentFontSize;else max2=this.currentFontSize}}}var fit=!(poc.height>this.height||poc.width>this.width);found=fit&&min>=max-1;if(fit)min=this.currentFontSize;else max=this.currentFontSize}}poc._style=oldStyle;
