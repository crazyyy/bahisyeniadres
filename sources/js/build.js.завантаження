Renderer.prototype.initMaskTextures=function(){this.RTunder=new PIXI.RenderTexture(this.renderer,this.renderer.width,this.renderer.height);this.RTmask=new PIXI.RenderTexture(this.renderer,this.renderer.width*this.MaskResize,this.renderer.height*this.MaskResize);this.RTclipped=new PIXI.RenderTexture(this.renderer,this.renderer.width,this.renderer.height);this.RTfinal=new PIXI.RenderTexture(this.renderer,this.renderer.width,this.renderer.height);this.clipMaskFilter=new CLIPMASKFilter(this.RTunder,this.RTmask);
