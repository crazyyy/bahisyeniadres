AnimationCurve.prototype.Evaluate=function(animTime){var undefined;var time=animTime;var len=this.keys.length;if(len===1)return this.keys[0].value;var i,k0,k1;this._lastKey=Math.min(this._lastKey,len-1);if(time<=this.keys[0].time)return this.keys[0].value;else if(time>=this.keys[len-1].time)return this.keys[len-1].value;var start=this.keys[this._lastKey].time<=time?this._lastKey:0;for(i=start;i<len-1;i++)if(this.keys[i].time<=time&&this.keys[i+1].time>time){k0=this.keys[i];k1=this.keys[i+1];this._lastKey=
i;break}if(!isFinite(k0.outTangent)||!isFinite(k1.inTangent))return k0.value;var t=UHTMath.inverseLerp(k0.time,k1.time,time);var dt=k1.time-k0.time;var m0=k0.outTangent*dt;var m1=k1.inTangent*dt;var t2=t*t;var t3=t2*t;var a=2*t3-3*t2+1;var b=t3-2*t2+t;var c=t3-t2;var d=-2*t3+3*t2;return a*k0.value+b*m0+c*m1+d*k1.value};
