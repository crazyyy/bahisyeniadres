var EventManager={handlers:{},queuedEvts:[],queuedArgs:[],Init:function(){},AddHandler:function(eventName,callback,object){if(EventManager.handlers[eventName]==undefined)EventManager.handlers[eventName]=[];EventManager.handlers[eventName].push(new EventHandler(object,callback))},ClearCallback:function(callback,object){for(var eventName in EventManager.handlers){if(!EventManager.handlers.hasOwnProperty(eventName))continue;for(var i=0;i<EventManager.handlers[eventName].length;++i)if(EventManager.handlers[eventName][i].equals(object,
