AnimatedParticleFrames.prototype.BuildSprites=function(){var mSpriteNames=this.atlas.getSpriteNames();mSpriteNames.sort(function(a,b){return a==b?0:a>b?1:-1});for(var i=0;i<mSpriteNames.length;i++)this.frameInfo.push(this.atlas.getTextureForSimpleSprite(null,mSpriteNames[i]))};AnimatedParticleFrames.prototype.SupportsFastParticleContainer=function(){return false};
