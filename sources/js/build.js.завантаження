function SoundPlayer(){Component.call(this);this.audioClip=null;this.whenToPlay=WhenToPlay.Custom;this.playMode=PlayMode.OneShot;this.playOnlyIfEnabled=false;this.muteBackgroundMusic=false;this.handler=-1;this.canPlayHoverSound=true}SoundPlayer.prototype.PlaySound=function(){this.InternalPlayClip(this.audioClip)};
